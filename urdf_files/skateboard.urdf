<robot name="skateboard">

    <link name="deck">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="2.032 6.096 0.127"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="2.5"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="2.032 6.096 0.127"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu value="10"/>
                        <mu2 value="10"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="deck_curve1">
        <visual>
            <origin xyz="0 3.556 0.2" rpy="0.375 0 0"/>
            <geometry>
                <box size="2.032 1.016 0.127"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 3.556 0.2" rpy="0.375 0 0"/>
            <mass value="0.4032"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 3.556 2" rpy="0.375 0 0"/>
            <geometry>
                <box size="2.032 1.016 0.127"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu value="10"/>
                        <mu2 value="10"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="deck_curve2">
        <visual>
            <origin xyz="0 -3.556 0.2" rpy="-0.375 0 0"/>
            <geometry>
                <box size="2.032 1.016 0.127"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 -3.556 0.2" rpy="-0.375 0 0"/>
            <mass value="0.4032"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 -3.556 2" rpy="-0.375 0 0"/>
            <geometry>
                <box size="2.032 1.016 0.127"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu value="10"/>
                        <mu2 value="10"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="steering">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
            <material name="white">
                <color rgba="0.8 1 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.2" length="0.1"/>
            </geometry>
        </collision>
    </link>

    <link name="front_left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry>
            <material name="white">
                <color rgba="0.8 1 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry> 
            <surface>
                <friction>
                    <ode>
                        <mu value="3"/>
                        <mu2 value="1"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="front_right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry>
            <material name="white">
                <color rgba="0.8 1 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry> 
            <surface>
                <friction>
                    <ode>
                        <mu value="3"/>
                        <mu2 value="1"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="back_left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry>
            <material name="white">
                <color rgba="0.8 1 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry> 
            <surface>
                <friction>
                    <ode>
                        <mu value="3"/>
                        <mu2 value="1"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>

    <link name="back_right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry>
            <material name="white">
                <color rgba="0.8 1 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.2"/>
            <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
        </inertial>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.27" length="0.35"/>
            </geometry> 
            <surface>
                <friction>
                    <ode>
                        <mu value="3"/>
                        <mu2 value="1"/>
                    </ode>
                </friction>
            </surface>
        </collision>
    </link>


    <joint name="deck_curve1_joint" type="fixed">
        <parent link="deck"/>
        <child link="deck_curve1"/>
    </joint>

    <joint name="deck_curve2_joint" type="fixed">
        <parent link="deck"/>
        <child link="deck_curve2"/>
    </joint>

    <joint name="steering_joint" type="revolute">
        <origin xyz="0 0 -0.2" rpy="0 0 0"/>
        <parent link="deck"/>
        <child link="steering"/>
        <axis xyz="0 1 0"/>
        <limit lower="-0.1745" upper="0.1745" velocity="0.3"/>
    </joint>


    <joint name="front_left_wheel_joint" type="continuous">
        <origin xyz="0.72 2.4 -0.34" rpy="0 1.570796 0"/>
        <axis xyz="0 0 1"/>
        <parent link="steering"/>
        <child link="front_left_wheel"/>
    </joint>

    <joint name="front_right_wheel_joint" type="continuous">
        <origin xyz="-0.72 2.4 -0.34" rpy="0 1.570796 0"/>
        <axis xyz="0 0 1"/>
        <parent link="steering"/>
        <child link="front_right_wheel"/>
    </joint>

    <joint name="back_left_wheel_joint" type="continuous">
        <origin xyz="0.72 -2.4 -0.34" rpy="0 1.570796 0"/>
        <axis xyz="0 0 1"/>
        <parent link="steering"/>
        <child link="back_left_wheel"/>
    </joint>

    <joint name="back_right_wheel_joint" type="continuous">
        <origin xyz="-0.72 -2.4 -0.34" rpy="0 1.570796 0"/>
        <axis xyz="0 0 1"/>
        <parent link="steering"/>
        <child link="back_right_wheel"/>
    </joint>

</robot>